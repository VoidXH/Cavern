<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<OutputType>Library</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<Authors>VoidX</Authors>
		<Description>Models and algorithms for the operation of the Cavernize conversion process.</Description>
		<Copyright>Copyright © Bence Sgánetz 2016-2025</Copyright>
		<PackageProjectUrl>https://cavern.sbence.hu/</PackageProjectUrl>
		<RepositoryUrl>https://github.com/VoidXH/Cavern</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>audio;sound;rendering;spatial audio;surround sound</PackageTags>
		<NeutralLanguage>en</NeutralLanguage>
		<Nullable>disable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>
	<ItemGroup>
		<Compile Include="..\..\VoidX.WPF\FFmpeg\FFmpeg.cs" Link="VoidX.WPF\FFmpeg\FFmpeg.cs" />
		<Compile Include="..\..\VoidX.WPF\FFmpeg\FFmpegArgument.cs" Link="VoidX.WPF\FFmpeg\FFmpegArgument.cs" />
		<Compile Include="..\..\VoidX.WPF\FFmpeg\FFmpegConfigurator.cs" Link="VoidX.WPF\FFmpeg\FFmpegConfigurator.cs" />
		<Compile Include="..\..\VoidX.WPF\FFmpeg\FFmpegStream.cs" Link="VoidX.WPF\FFmpeg\FFmpegStream.cs" />
		<Compile Include="..\..\VoidX.WPF\HTTP.cs" Link="VoidX.WPF\HTTP.cs" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\..\Cavern\Cavern.csproj" />
		<ProjectReference Include="..\..\..\Cavern.Format\Cavern.Format.csproj" />
	</ItemGroup>
</Project>
