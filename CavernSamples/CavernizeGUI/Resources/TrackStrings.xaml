<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- Export formats -->
    <system:String x:Key="C_AC3">AC-3 (mediocre, supports SPDIF)</system:String>
    <system:String x:Key="CEAC3">Enhanced AC-3 (mediocre, supports HDMI ARC)</system:String>
    <system:String x:Key="COpus">Opus (transparent, small size)</system:String>
    <system:String x:Key="CFLAC">FLAC (lossless, large size)</system:String>
    <system:String x:Key="CPCMI">PCM, integer (lossless, larger size)</system:String>
    <system:String x:Key="CPCMF">PCM, float (needless, largest size)</system:String>
    <system:String x:Key="CADMC">ADM Broadcast Wave Format (compact)</system:String>
    <system:String x:Key="CADMA">ADM Broadcast Wave Format (Dolby Atmos)</system:String>
    <system:String x:Key="C_LAF">Limitless Audio Format</system:String>
</ResourceDictionary>